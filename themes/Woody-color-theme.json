			"scope": [
				"support.type.vendored.property-name",
				"support.type.property-name",
				"entity.name.type",
				"variable.css",
				"variable.scss",
				"variable.other.less"
			],
			"settings": {
				"foreground": "#E6844F"
			}
		},
