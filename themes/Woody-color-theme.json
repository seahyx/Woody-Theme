			"scope": "meta.diff.header",
			"settings": {
				"foreground": "#58b2dc"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#58b2dc"
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#4EC9B0"
			}
		},
		{
			"scope": "storage.type.annotation",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": ["storage.modifier"],
			"settings": {
				"foreground": "#58b2dc"
			}
		},
		{
			"scope": "string.tag",
			"settings": {
				"foreground": "#ce9178"
			}
		},
		{
			"scope": "string.value",
			"settings": {
				"foreground": "#ce9178"
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#d16969"
			}
		},
