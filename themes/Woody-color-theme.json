			"scope": ["keyword.other.unit", "keyword.other.using"],
			"settings": {
				"foreground": "#dd89b1"
			}
		},
		{
			"scope": [
				"punctuation.section.embedded.begin.php",
				"punctuation.section.embedded.end.php"
			],
			"settings": {
				"foreground": "#58b2dc"
			}
		},
