	"tokenColors": [
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "header",
			"settings": {
				"foreground": "#000080"
			}
		},
		{
			"scope": [
				"comment",
				"entity.name.tag.localname",
				"keyword.preprocessor",
				"meta.preprocessor"
			],
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"scope": ["comment"],
			"settings": {
				"foreground": "#919191"
			}
		},
		{
			"scope": "string.unquoted.preprocessor",
			"settings": {
				"foreground": "#a1a1a1",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#dc589a"
			}
		},
		{
			"scope": ["constant.numeric"],
			"settings": {
				"foreground": "#986DB2"
			}
		},
		{
			"scope": "constant.regexp",
			"settings": {
				"foreground": "#646695"
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
